{
  "name": "serialport-meta",
  "private": true,
  "version": "0.0.0",
  "description": "Node.js packages to access serial ports, process data from them and speak many protocols",
  "engines": {
    "node": ">=4.0.0"
  },
  "license": "MIT",
  "scripts": {
    "docs": "jsdoc -c ./.jsdoc.json ",
    "build-from-source": "npx lerna exec --scope serialport -- npm i --build-from-source",
    "exec-test": "lerna exec node ./",
    "lint": "eslint packages && lerna exec --scope serialport -- 'npm run lint'",
    "postinstall": "lerna bootstrap",
    "publish": "lerna publish",
    "test": "nyc --reporter=html --reporter=text --reporter lcovonly mocha",
    "test:watch": "mocha -w",
    "prebuild": "lerna exec --scope serialport -- 'npm run prebuild'",
    "prebuild-ci": "lerna exec --scope serialport -- 'npm run prebuild-ci'"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/node-serialport/node-serialport.git"
  },
  "maintainers": [
    {
      "name": "Francis Gulotta",
      "email": "wizard@roborooter.com",
      "url": "https://www.roborooter.com"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "@reconbot/jsdoc-theme": "^1.0.0",
    "bluebird": "^3.5.0",
    "cc": "^1.0.1",
    "chai": "^4.0.2",
    "chai-subset": "^1.5.0",
    "eslint": "^5.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "proxyquire": "^2.0.0",
    "sinon": "^6.0.1",
    "lerna": "^2.11.0",
    "nyc": "^13.0.0",
    "plop": "^2.0.0"
  }
}
